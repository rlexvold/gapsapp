{
    "env": {
        "browser": true, // browser global variables.
        "node": true, // Node.js global variables and Node.js-specific rules.
        "amd": false, // defines require() and define() as global variables as per the amd spec.
        "mocha": true, // adds all of the Mocha testing global variables.
        "jasmine": false, // adds all of the Jasmine testing global variables for version 1.3 and 2.0.
        "phantomjs": false, // phantomjs global variables.
        "jquery": false, // jquery global variables.
        "prototypejs": false, // prototypejs global variables.
        "shelljs": false // shelljs global variables.
    },
    "globals": {
        "Promise": true,
        "process": false,
        "require": false,
        "define": false,
        "console": false
    },
    "plugins": [
        // e.g. "react" (must run `npm install eslint-plugin-react` first)
        "react",
        "chai-expect",
        "mocha"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "jsx": true,
            "classes": true,
            "modules": true
        }
    },
    "rules": {
        "comma-dangle": [
            "error",
            "never"
        ],
        "no-cond-assign": [
            1,
            "always"
        ],
        "no-console": 2,
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "no-debugger": 1,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 2,
        "no-empty": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": [
            2,
            "all"
        ],
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-inner-declarations": [
            1,
            "both"
        ],
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 2,
        "no-sparse-arrays": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-jsdoc": 0,
        "valid-typeof": 2,
        "no-unexpected-multiline": 2,
        "accessor-pairs": [
            1, {
                "setWithoutGet": true
            }
        ],
        "block-scoped-var": 2,
        "complexity": 0,
        "consistent-return": 0,
        "curly": [
            2,
            "all"
        ],
        "default-case": 2,
        "dot-notation": 0,
        "dot-location": 0,
        "eqeqeq": [
            2,
            "smart"
        ],
        "guard-for-in": 2,
        "no-alert": 2,
        "no-caller": 2,
        "no-div-regex": 2,
        "no-else-return": 2,
        "no-empty-label": 0,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 0,
        "no-extra-bind": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-implicit-coercion": 0,
        "no-implied-eval": 2,
        "no-invalid-this": 2,
        "no-iterator": 2,
        "no-labels": 0,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-spaces": 0,
        "no-multi-str": 2,
        "no-native-reassign": 0,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-new": 2,
        "no-octal-escape": 0,
        "no-octal": 2,
        "no-param-reassign": 0,
        "no-process-env": 2,
        "no-proto": 0,
        "no-redeclare": [
            2, {
                "builtinGlobals": true
            }
        ],
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-unused-expressions": 2,
        "no-useless-call": 2,
        "no-void": 2,
        "no-warning-comments": 0,
        "no-with": 0,
        "radix": 0,
        "vars-on-top": 2,
        "wrap-iife": 0,
        "yoda": 0,
        "strict": 0,
        "init-declarations": 0,
        "no-catch-shadow": 0,
        "no-delete-var": 2,
        "no-label-var": 2,
        "no-shadow-restricted-names": 0,
        "no-shadow": 0,
        "no-undef-init": 0,
        "no-undef": 2,
        "no-undefined": 0,
        "no-unused-vars": 2,
        "no-use-before-define": 0,
        "callback-return": 0,
        "handle-callback-err": 0,
        "no-mixed-requires": 0,
        "no-new-require": 0,
        "no-path-concat": 0,
        "no-process-exit": 0,
        "no-restricted-modules": 0,
        "no-sync": 0,
        "array-bracket-spacing": 0,
        "brace-style": [
            2,
            "1tbs", {
                "allowSingleLine": true
            }
        ],
        "camelcase": [
            2, {
                "properties": "always"
            }
        ],
        "comma-spacing": 0,
        "comma-style": 2,
        "computed-property-spacing": 0,
        "consistent-this": 0,
        "eol-last": 0,
        "func-names": 0,
        "func-style": 0,
        "id-length": 0,
        "indent": [
            2,
            4
        ],
        "key-spacing": 0,
        "lines-around-comment": 0,
        "linebreak-style": 0,
        "max-nested-callbacks": 0,
        "new-cap": 0,
        "new-parens": 0,
        "newline-after-var": 0,
        "no-array-constructor": 0,
        "no-continue": 0,
        "no-inline-comments": 0,
        "no-lonely-if": 0,
        "no-mixed-spaces-and-tabs": [
            2,
            "smart-tabs"
        ],
        "no-multiple-empty-lines": [
            2, {
                "max": 2
            }
        ],
        "no-nested-ternary": 0,
        "no-new-object": 0,
        "no-spaced-func": 0,
        "no-ternary": 0,
        "no-trailing-spaces": [
            2, {
                "skipBlankLines": false
            }
        ],
        "no-underscore-dangle": 0,
        "no-unneeded-ternary": 0,
        "object-curly-spacing": 0,
        "one-var": 0,
        "operator-assignment": 0,
        "operator-linebreak": 0,
        "padded-blocks": 0,
        "quote-props": 0,
        "quotes": [
            2,
            "single",
            "avoid-escape"
        ],
        "id-match": 0,
        "semi-spacing": 0,
        "semi": [2, "never"],
        "sort-vars": 0,
        "space-after-keywords": 0,
        "space-before-blocks": 0,
        "space-before-function-paren": 0,
        "space-in-parens": 0,
        "space-infix-ops": 0,
        "space-return-throw-case": 0,
        "space-unary-ops": 0,
        "spaced-comment": 0,
        "wrap-regex": 0,
        "arrow-parens": 0,
        "arrow-spacing": 0,
        "constructor-super": 0,
        "generator-star-spacing": 0,
        "no-class-assign": 0,
        "no-const-assign": 0,
        "no-this-before-super": 0,
        "no-var": 0,
        "object-shorthand": 0,
        "prefer-const": 0,
        "prefer-spread": 0,
        "prefer-reflect": 0,
        "require-yield": 0,
        "max-depth": 0,
        "max-len": 0,
        "max-params": 0,
        "max-statements": 0,
        "no-bitwise": 0,
        "no-plusplus": 0,
        "react/jsx-uses-react": 2,
        "react/jsx-uses-vars": 2,
        "react/react-in-jsx-scope": 2

    }
}